{
  "name": "@metinseylan/nestjs-opentelemetry",
  "version": "2.0.3",
  "description": "deeply integrated OpenTelemetry module for Nestjs",
  "author": "metinsyln@gmail.com",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MetinSeylan/Nestjs-OpenTelemetry.git"
  },
  "keywords": [
    "nestjs",
    "opentelemetry",
    "tracing",
    "observability",
    "metric",
    "prometheus",
    "zipkin",
    "jaeger"
  ],
  "bugs": {
    "url": "https://github.com/MetinSeylan/Nestjs-OpenTelemetry/issues"
  },
  "homepage": "https://github.com/MetinSeylan/Nestjs-OpenTelemetry#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.1.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/microservices": "^8.1.1",
    "@nestjs/schedule": "^1.0.2",
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/context-async-hooks": "^1.0.1",
    "@opentelemetry/core": "^1.0.1",
    "@opentelemetry/host-metrics": "^0.27.1",
    "@opentelemetry/instrumentation-http": "^0.27.0",
    "@opentelemetry/propagator-b3": "^1.0.1",
    "@opentelemetry/propagator-jaeger": "^1.0.1",
    "@opentelemetry/resource-detector-aws": "^1.0.3",
    "@opentelemetry/resources": "^1.0.1",
    "@opentelemetry/sdk-node": "^0.27.0",
    "@opentelemetry/sdk-trace-base": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.7",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.2",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.5.2",
    "wait-for-expect": "^3.0.2"
  }
}
